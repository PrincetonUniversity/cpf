@InProceedings{onder:98:ICCL,
  author =    {"Soner {\"{O}}nder and Rajiv Gupta},
  title =    {Automatic Generation of Microarchitecture Simulators},
  booktitle =    "Proceedings of the 1998 " # ICCL ,
  OPTcrossref =  {},
