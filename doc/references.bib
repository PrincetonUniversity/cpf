@article{zhou:10:taco, title={Performance-aware
thermal management via task scheduling}, volume={7},
url={http://portal.acm.org/citation.cfm?id=1736065.1736070},
abstractNote={High on-chip temperature impairs the processorʼs
reliability and reduces its lifetime. Hardware-level dynamic thermal
management (DTM) techniques can effectively constrain the chip
temperature, but degrades the performance. We propose an OS-level
technique that performs thermal-aware job scheduling to reduce DTMs.
The algorithm is based on the observation that hot and cool jobs
executed in a different order can make a difference in resulting
temperature. Real-system implementation in Linux shows that our
scheduler can remove 10.5&percnt; to 73.6&percnt; of the hardware
DTMs in a medium thermal environment. The CPU throughput is improved
by up to 7.6&percnt; (4.1&percnt;, on average) in a severe thermal
environment.}, number={1}, journal={ACM Transactions on Architecture
and Code Optimization TACO}, author={Zhou, X and Yang, J and
Chrobak, M and Zhang, Y}, year={2010}, pages={1-31}}

@article{jayaseelan:08:cad, title={Temperature aware task
sequencing and voltage scaling},
url={http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=4681641},
abstractNote={On-chip power density and temperature are rising
exponentially with decreasing feature sizes. This alarming trend
calls for temperature management at every level of system design. In
this paper, we propose task sequencing as a powerful and
complimentary mechanism to voltage scaling in improving the thermal
profile of an embedded system executing a set of periodic
heterogenous tasks under timing constraints. We first derive the
peak temperature of a repeating task sequence analytically and
develop a heuristic to construct the task sequence that minimizes
the peak temperature. Experimental evaluation shows that our task
sequencing heuristic achieves peak temperature within 0.5C of the
optimal solution and 7.47C lower, on an average, compared to the
worst sequence for a large range of embedded task sets. We also
propose an iterative algorithm that combines task sequencing with
voltage scaling to further lower the peak temperature while
satisfying the timing constraints. For embedded task sets, our
combined task sequencing and voltage scaling approach achieves, on
an average, 2.1C - 6.94C reduction in peak temperature compared to
voltage scaling alone.}, journal={2008 IEEE/ACM International
Conference on Computer Aided Design}, publisher={Ieee},
author={Jayaseelan, Ramkumar and Mitra, Tulika}, year={2008},
pages={618-623}}

